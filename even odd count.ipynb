    "n = int(input(\"Enter the number:\"))\n",
    "\n",
    "odd_list = []\n",
    "even_list = []\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    if i%2 == 0:\n",
    "        even_list.append(i)\n",
    "    else:\n",
    "        odd_list.append(i)\n",
    "print(\"even_list:\", even_list)\n",
    "print(\"odd_list:\", odd_list)\n",
    "    \n"
